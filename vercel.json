{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "research-viewer/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/dashboard/(.*)",
      "dest": "dashboard/api/index.js"
    },
    {
      "src": "/research/(.*)",
      "dest": "research-viewer/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dashboard/api/index.js"
    }
  ]
}
